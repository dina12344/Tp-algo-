#include<stdio.h>
int main(){
int n,i,j,temp;
// قراءة حجم مصفوفة المربع
printf("أدخل حجم مصفوفة المربع (n * n):");
scanf("%d",&n);

int a[10][10]؛

// قراءة عناصر المصفوفة
printf("أدخل العناصر:\n");
for(i=0;i<n;i++)
for(j=0;j<n;j++)
scanf("%d",&a[i][j]);
//نقل المصفوفة عبر القطر الرئيسي
for(i=0;i<n;i++)
for(j=0;j<n;j++){
temp=a[i][j];
a[i][j]=a[j][i];
a[j][i]=temp;
}

//اطبع المصفوفة المنقولة
printf("\nTransposed matrix:\n");
for(i=0;i<n;i++){
for(j=0;j<n;j++)
printf("%d",a[i][j]);
printf("\n");
}
return 0;
}
